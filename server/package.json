{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "bin": "server.js",
  "type": "commonjs",
  "engines": {
    "node": ">=18"
  },
  "pkg": {
    "targets": [
      "node18-linux-x64"
    ],
    "outputPath": "dist",
    "scripts": [
      "server.js",
      "routes/**/*.js",
      "models/**/*.js",
      "middleware/**/*.js",
      "utils/**/*.js",
      "node_modules/axios/dist/node/axios.cjs",
      "node_modules/axios/index.js",
      "node_modules/dotenv/lib/main.js"
    ],
    "assets": [
      "public/**/*",
      "node_modules/axios/package.json",
      "node_modules/dotenv/package.json"
    ]
  },
  "scripts": {
    "start": "nodemon server.js",
    "build": "pkg ."
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.18.0",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.25",
    "ldapjs": "^3.0.7",
    "mongoose": "^8.7.1",
    "multer": "^2.0.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.1",
    "pdfkit": "^0.17.1",
    "pdfkit-table": "^0.1.99",
    "pkg": "^5.8.1",
    "splunk-sdk": "^2.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.7"
  }
}
